<keymap version="1" name="Windows Proper Redo" parent="$default">
  <action id="$Redo">
    <keyboard-shortcut first-keystroke="shift ctrl z" />
    <keyboard-shortcut first-keystroke="shift alt back_space" />
    <keyboard-shortcut first-keystroke="ctrl y" />
  </action>
  <action id="CommentByBlockComment">
    <keyboard-shortcut first-keystroke="shift ctrl slash" />
  </action>
  <action id="CommentByLineComment">
    <keyboard-shortcut first-keystroke="ctrl slash" />
  </action>
  <action id="Console.Execute.Multiline">
    <keyboard-shortcut first-keystroke="f5" />
  </action>
  <action id="CopyElement" />
  <action id="EditorDeleteLine">
    <keyboard-shortcut first-keystroke="ctrl l" />
  </action>
  <action id="FindNext">
    <keyboard-shortcut first-keystroke="f3" />
  </action>
  <action id="Graph.RouteEdges" />
  <action id="Vcs.Log.FocusTextFilter" />
</keymap>